<?php

namespace Como\FormBuilderBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Como\FormBuilderBundle\Model\Widget as BaseWidget;

/**
 * Widget
 */
class Widget extends BaseWidget
{
    /**
     * @var integer
     */
    private $id;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
